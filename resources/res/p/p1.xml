<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <gradient android:startColor="@color/r7" android:endColor="@color/a8w" android:angle="0" android:type="radial" android:gradientRadius="400" android:centerColor="@color/r7"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/ano"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</layer-list>
